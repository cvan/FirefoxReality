{
  "manifest_version": 2,
  "name": "FirefoxRealityYouTubeWebCompat",
  "version": "1.0",
  "description": "YouTube web-site compatability fixes for Firefox Reality.",
  "content_scripts": [
    {
      "matches": [
        "*://*.youtube.com/*",
        "*://*.youtube-nocookie.com/*"
      ],
      "css": [
        "main.css"
      ],
      "js": [
        "main.js"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ]
}
